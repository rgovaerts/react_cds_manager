[
    {
        "httpRequest": {
            "method": "post",
            "path": "/login",
            "body": {
                "username": "username",
                "password": "password"
            }
        },
        "httpResponse": {
            "body": {
                "token": "someToken"
            }
        }
    },
    {
        "httpRequest": {
            "path": "/cds-services",
            "headers": {
                "Authorization": [
                    "Bearer someToken"
                ]
            }
        },
        "httpResponse": {
            "delay": {
                "timeUnit": "SECONDS",
                "value": 1
            },
            "statusCode": 200,
            "headers": [
                {
                    "name": "Content-Type",
                    "values": ["application/json; charset=utf-8"]
                }
            ],
            "body": {
                "services": [
                    {
                        "id": "1",
                        "hook": "patient-view",
                        "title": "Patient view",
                        "description": "Displays which patient is viewed"
                    },
                    {
                        "id": "2",
                        "hook": "appointment-book",
                        "title": "Availibilty checker",
                        "description": "Checks the agenda for availability"
                    }
                ]
            }
        }
    }
]